//--------------------------------------------------------------------
//■LOOP
//--------------------------------------------------------------------
#=ES.CONFIG.LOOP
{
	LOOP[SET=1]
	{
		IF[@es.STATE==10]
		{
			\BT.ACTIVEGROUP(@activeno)

//			\_gosub(ES.CONFIG.VoicePlay.1)	//ボイス再生
			@es.STATE=11
		}
		IFEND[]
		IF[@es.STATE==11]
		{
			@es.STATE=12
		}
		IFEND[]
		IF[@es.STATE==12]
		{
			GOSUB[#=ES.CONFIG.INITIALIZE]
			@es.STATE=13
			LOOPBREAK[]
		}
		IFEND[]
		IF[@es.STATE==13] //FADE
		{
			\SS.FADE(250)
			@es.STATE=100
		}
		IFEND[]
		IF[@es.STATE==100] //LOOP
		{
			//----------------------------------- サンプルテキスト表示処理
			//
			GOSUB[#=ES.CONFIG.SMTX.EXEC]

			//----------------------------------- ボイス再生終了チェック
			//
			IF[@VOICEPLAYFLAG.SE] \SE.PCHK(3) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.SE.TEST",, 0);@VOICEPLAYFLAG.SE=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.ESE] \ESE.PCHK(3) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.ESE.TEST",, 0);@VOICEPLAYFLAG.ESE=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.E2SE] \E2SE.PCHK(3) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.E2SE.TEST",, 0);@VOICEPLAYFLAG.E2SE=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.SSE] \SSE.PCHK(3) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.SSE.TEST",, 0);@VOICEPLAYFLAG.SSE=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.S2SE] \S2SE.PCHK(3) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.S2SE.TEST",, 0);@VOICEPLAYFLAG.S2SE=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.VO] \VO.PCHK(10) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.VO.TEST",, 0);@VOICEPLAYFLAG.VO=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.EVO] \EVO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.EVO.TEST",, 0);@VOICEPLAYFLAG.EVO=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.E2VO] \E2VO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.E2VO.TEST",, 0);@VOICEPLAYFLAG.E2VO=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.SVO] \SVO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.SVO.TEST",, 0);@VOICEPLAYFLAG.SVO=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.S2VO] \S2VO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.S2VO.TEST",, 0);@VOICEPLAYFLAG.S2VO=0 IFEND[] IFEND[]

			//キャラ別
			IF[@VOICEPLAYFLAG.VOC] \VO.PCHK(10) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.CHARA.TEST", @VOICEPLAYFLAG.VOC, 0);@VOICEPLAYFLAG.VOC=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.EVOC] \EVO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.CHARA.EVO.TEST", @VOICEPLAYFLAG.EVOC, 0);@VOICEPLAYFLAG.EVOC=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.E2VOC] \E2VO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.CHARA.E2VO.TEST", @VOICEPLAYFLAG.E2VOC, 0);@VOICEPLAYFLAG.E2VOC=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.SVOC] \SVO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.CHARA.SVO.TEST", @VOICEPLAYFLAG.SVOC, 0);@VOICEPLAYFLAG.SVOC=0 IFEND[] IFEND[]
			IF[@VOICEPLAYFLAG.S2VOC] \S2VO.PCHK(5) IF[@_RINT(1)==0] \BT.ONOFF($BID+"VOL.CHARA.S2VO.TEST", @VOICEPLAYFLAG.S2VOC, 0);@VOICEPLAYFLAG.S2VOC=0 IFEND[] IFEND[]

		}
		IFEND[]

		IF[@es.STATE==200] //コンフィグ設定初期化
		{
			GOSUB[#=ES.CONFIG.INITEXEC]
			GOSUB[#=ES.CONFIG.INITIALIZE]

			\BT.END(ES.CONFIG)
			\BTDEF.LOAD(ES.CONFIG)

			GOSUB[#=ES.CONFIG.REF]

	//		GOSUB[#=ES.CONFIG.REF]
	//		GOSUB[#=es.BT.DRAW]	//ボタン描画 (設定初期化後のフラッシュ防止とかに必要)

			@es.STATE=100
			\SS.FADE(10)
			LOOPCONTINUE[]
		}
		IFEND[]
		IF[@es.STATE==201] //コンフィグ設定初期化(DIALOG)
		{
			GOSUB[#=ES.CONFIG.INITEXEC]
			GOSUB[#=ES.CONFIG.INITIALIZE]

			\BT.END(ES.CONFIG)
			\BTDEF.LOAD(ES.CONFIG)

			GOSUB[#=ES.CONFIG.REF]

			@es.STATE=100
			LOOPCONTINUE[]
		}
		IFEND[]

		IF[@es.STATE==202] //コンフィグ設定初期化確認から戻った
		{
			IF[@es.S.RINT(1)==1] @es.STATE=200 IFEND[] //OK
			IF[@es.S.RINT(1)==0] @es.STATE=901 IFEND[] //CANCEL
		}
		IFEND[]

		IF[@es.STATE==901] //設定初期化/タイトル確認/終了確認/シーン鑑賞確認からBACK
		{
			@es.STATE=12 //CANCEL
			LOOPCONTINUE[]
		}
		IFEND[]

		IF[@es.STATE==998] //フォント選択からBACK
		{
			GOSUB[#=ES.CONFIG.INITIALIZE]
			@es.STATE=100
		//	\SS.FADE(0) //一瞬ボタンが見えてしまう。
			\es.CGTDEL //こっちにする。
			LOOPCONTINUE[]
		}
		IFEND[]
	}
	LOOPEND[]

	return[]
}


//--------------------------------------------------------------------
//■コンフィグ画面 Initialize
//--------------------------------------------------------------------
#=ES.CONFIG.INITIALIZE
{
	//サンプルテキスト
	STR[$tmp="これはサンプルテキストです。自由に設定してください。"]

	\_strlen($tmp);INT[@len=@_RINT(1)]
	LOOP[SET=@len] \_strmid($tmp,@_LC,1)($SMTX.STR(@_LC)) LOOPEND[]
//	$SMTX.STR(15)+=" "

	//サンプルテキスト用レイヤ生成
	CGEND[ID=SMTX ]
	CGEND[ID=SMTX2]
	CG[ID=SMTX  Z=@es.Z.BT+2000 SX=600 SY=100 X=060 Y=-600 FILE=""]
	CG[ID=SMTX2 Z=@es.Z.BT+2000 SX=600 SY=100 X=100 Y=-600 FILE=""]
	CGACT[ID=SMTX  RECTPAINT3=1 SET=0]
	CGACT[ID=SMTX2 RECTPAINT3=1 SET=0]

		\FONT.COLOR( 0xffffff )
		\FONT.SIZE( 19 )
		\FONT.XY( 10, 10 )
		\FONT.BOLD( 1 )
	//	\FONT.SHADE( 2, 2, 0x000000 )
		\FONT.OUTLINE( 0x000000 )
	//	\FONT.COLOR( 0xffffff, 0x6699bb )
		\FONT.DRAW("SMTX2", "このウィンドウはバックログ用のサンプルです。")

		\FONT.COLOR( 0xffffff )
		\FONT.SIZE( 19 )
		\FONT.XY( 10, 10+19+2 )
		\FONT.BOLD( 1 )
	//	\FONT.SHADE( 2, 2, 0x000000 )
		\FONT.OUTLINE( 0x000000 )
	//	\FONT.COLOR( 0xffffff, 0x6699bb )
		\FONT.DRAW("SMTX2", "背景透過度の設定のみ可能です。")

	GOSUB[#=ES.CONFIG.SMTX.INIT]

	GOSUB[#=ES.CONFIG.REF]

	\_gosub(es.Original.CfgInit)

	GOSUB[#=es.BT.DRAW]	//ボタン描画 (設定初期化後のフラッシュ防止とかに必要)

	return[]
}


