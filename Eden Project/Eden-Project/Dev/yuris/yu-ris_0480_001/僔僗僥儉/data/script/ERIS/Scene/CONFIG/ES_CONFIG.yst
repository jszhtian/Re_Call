//--------------------------------------------------------------------
//■設定初期化処理
//--------------------------------------------------------------------
#=ES.CONFIG.INITEXEC
{
	//----------------------------------------------------------------
	// ボイス再生
	//----------------------------------------------------------------
		\_gosub(ES.CONFIG.VoicePlay.3)

	//----------------------------------------------------------------
	// フォント表示
	//----------------------------------------------------------------
		@es.GSD(113,01) = 0
	//	GOSUB[#=es.FONT.NAME.GET pint=@es.GSD(113,01)]
	//	$T(36,11) = $_RSTR(1)  // デフォルトフォント
		$es.GSD(113) = $T(36,11)	// デフォルトフォント

	//----------------------------------------------------------------
	// 初期化
	//----------------------------------------------------------------
		IF[0]
		{
			GOSUB[#=es.GSD.INIT.CFG pint=0] //全初期化
		}
		ELSE[]
		{
			GOSUB[#=es.BT.ACTIVEGROUP.GET]
			GOSUB[#=es.GSD.INIT.CFG pint=@_RINT(1)] //タブ毎に初期化
		}
		IFEND[]

		GOSUB[#=ES.CONFIG.INITIALIZE]

		GOSUB[#=es.VOICE.DEL]

		\GSD.REFLECT
		GOSUB[#=es.SOUND.SETVOL]

	return[]
}


//--------------------------------------------------------------------
//■コンフィグ画面リフレッシュ
//--------------------------------------------------------------------
#=ES.CONFIG.REF
{
	GOSUB[#=es.BT.ACTIVEGROUP.GET];IF[@_RINT(1)>0] @activeno=@_RINT(1) IFEND[]
	\BT.ACTIVEGROUP(@activeno)
	\BT.ON($BID+"BTN.TAB", @activeno)

	///x必要か確認.
//	LOOP[SET=@es.CHAR.MAX] \BT.OFF($BID+"VOL.CHARA.MUTE.OFF", @_LC) LOOPEND[]

	//フォント表示
	GOSUB[#=es.FONT.Disabled]
	GOSUB[#=es.FONT.Disabled.2]
	IF[@activeno==@T(18,10)] GOSUB[#=es.FONT.Enabled] IFEND[]

	//サンプルテキスト
	IF[@activeno==@const.SAMPLETEXTTAB]
	{
		CG[ID=SMTX  E=1]
		CG[ID=SMTX2 E=1]
	}
	ELSE[]
	{
		CG[ID=SMTX  E=0]
		CG[ID=SMTX2 E=0]
	}
	IFEND[]

	return[]
}


//----------------------------------------------------------------
//■サンプルテキスト表示処理
//----------------------------------------------------------------
#=ES.CONFIG.SMTX.EXEC
{
//	LOOP[SET=31] @SMTX.FF(@_LC)=0 LOOPEND[]

	INT[@hx]
	INT[@hy]
	INT[@now]
	INT[@TX.SPD]
	INT[@TX.SX=22]
	INT[@TX.SY=22]

	//----------------------------------------------------------------
	// フォント変更
	//----------------------------------------------------------------
	//	\_txfont($LOG.TX.FONT.DEF)
	//	STR[$fonttemp = $_RSTR(1)]
//		GOSUB[#=es.TX.FONT.DEF.GET]
//		\_txfont($_RSTR(1))
//		\_txfont()
//		STR[$fonttemp = $_RSTR(1)]

//\_txsize(@TX.SX, @TX.SY)

	IF[@SMTX.FLAG==1]
	{
		LOOP[]
		{
			@now=@_OS_TIME(1)

			//GOSUB[#=es.TX.SPD.GET]
			@TX.SPD=@es.GSD(120,01)


			IF[@now-@SMTX.STARTTIME >= @TX.SPD * 100 / 256]
			{
//\_dmes($@TX.SPD)
				@SMTX.STARTTIME += @TX.SPD * 100 / 256
			//	@SMTX.STARTTIME-=(@now-@SMTX.STARTTIME-200)
//				@SMTX.FF(@SMTX.COUNT)=1
				@SMTX.COUNT+=1

				IF[@SMTX.COUNT<(15)]
					@hx=10+@TX.SX*(@SMTX.COUNT-   1);@hy=10+00
				ELSE[]
					@hx=10+@TX.SX*(@SMTX.COUNT-(15));@hy=10+(@TX.SY+5)
				IFEND[]

				IF[@SMTX.COUNT>=(27)]
				{
					//CG[ID=CFG.TX.GAIJI E=0 Z=1 FILE="cgsys/main/gaiji_01_24"]
					//CGACT[MERGE2=1 ID=CFG.TX.GAIJI ID2=SMTX2 X=@hx+1 Y=@hy+1]
					//CGEND[ID=TX.GAIJI]

					@SMTX.COUNT=@_OS_TIME(1)
					@SMTX.FLAG=2
					LOOPBREAK[]
				}
				IFEND[]


				\FONT.COLOR( 0xffffff )
				\FONT.SIZE( 22 )
				\FONT.XY( @hx , @hy )
				IF[@es.GSD(115,14)==1] \FONT.BOLD( 1 )						IFEND[]
				IF[@es.GSD(115,09)==1] \FONT.SHADE( 1, 1, 0x000000 )		IFEND[]
				IF[@es.GSD(115,08)==1] \FONT.OUTLINE( 0x000000 )			IFEND[]
				IF[@es.GSD(115,15)==1] \FONT.COLOR( 0xffffff, 0x6699bb )	IFEND[]
				\FONT.DRAW("SMTX", $SMTX.STR(@SMTX.COUNT))

				LOOPCONTINUE[]
			}
			ELSE[]
			{
				LOOPBREAK[]
			}
			IFEND[]

		}
		LOOPEND[]

	}
	IFEND[]

	IF[@SMTX.FLAG==2]
	{
		IF[@_OS_TIME(1)-@SMTX.COUNT > (@es.GSD(120,02)+25)*20] //500〜5620ms
		{
			GOSUB[#=ES.CONFIG.SMTX.INIT]
		}
		IFEND[]
	}
	IFEND[]


	//----------------------------------------------------------------
	// フォント元に戻す
	//----------------------------------------------------------------
//		\_txfont($fonttemp)

	return[]
}


//サンプルテキスト初期化
#=ES.CONFIG.SMTX.INIT
{
	@SMTX.FLAG		= 1
	@SMTX.COUNT		= 0
	@SMTX.STARTTIME	= @_OS_TIME(1)
	CGACT[ID=SMTX RECTPAINT3=1 SET=0]
	return[]
}

#=ES.CONFIG.FONT.REF
{
	GOSUB[#=ES.CONFIG.SMTX.INIT]	//サンプルテキスト初期化
	GOSUB[#=es.FONT.Enabled]		//フォント更新
	return[]
}



