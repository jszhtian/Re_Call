//
//#=ES.CONFIG.SCREEN.VDEF { return[] }
#=ES.CONFIG.SCREEN.INIT
{
///xS $es.BID,@es.BNO だと、GOSUB[#=ES.CONFIG.SCREEN.INIT] として直接飛んだときに、
//     違う内容が入っている場合があり、うまく動かない.
//
//	\BT.ONOFF($es.BID, 0, (@es.GSD(141,02)==1)) //1=ON
//	\BT.ONOFF($es.BID, 1, (@es.GSD(141,02)==0)) //0=OFF
	\BT.ONOFF("ES.CONFIG.SCREEN", 0, (@es.GSD(141,02)==1)) //1=ON
	\BT.ONOFF("ES.CONFIG.SCREEN", 1, (@es.GSD(141,02)==0)) //0=OFF
	return[]
}
#=ES.CONFIG.SCREEN.ON
{
	\BT.ONOFF.GET($es.BID, 0)(@es.GSD(141,02)) //1=ON／0=OFF
	WINDOW[NO=0 FULLSCREEN=@es.GSD(141,02)]
//	WINDOW[NO=0 Z=@es.GSD(141,02)] //前面or常に手前
	WINDOW[NO=0 Z=@es.GSD(110,01)] //前面or常に手前
	return[]
}

