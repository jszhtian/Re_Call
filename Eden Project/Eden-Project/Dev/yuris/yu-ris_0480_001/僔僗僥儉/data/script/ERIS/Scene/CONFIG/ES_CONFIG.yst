//■[キャラ別音量テスト] ---------------------------------------------
//
#=ES.CONFIG.VOL.CHARA.TEST.ON
{
	INT[@no=@_PINT(1)]

	IF[@VOICEPLAYFLAG.VOC!=@no] @VOICEPLAYFLAG.VOC=0 IFEND[]

	IF[@VOICEPLAYFLAG.VOC==0]
	{
		\BT.ONOFF($es.BID, @no, 1)
		\_gosub("ES.CONFIG.VoicePlay.VOC", @no)
		@VOICEPLAYFLAG.VOC = @no
	}
	ELSE[]
	{
		\BT.ONOFF($es.BID, @no, 0)
		GOSUB[#=es.VOICE.DEL] // ボイス停止
	//	\_gosub("ES.CONFIG.VoiceStop")
		@VOICEPLAYFLAG.VOC = 0
	}
	IFEND[]

	return[]
}

