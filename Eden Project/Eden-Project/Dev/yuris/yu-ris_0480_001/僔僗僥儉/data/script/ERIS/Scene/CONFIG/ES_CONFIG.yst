#=ES.CONFIG.VOL.CHARA.MUTE.ON.VDEF
#=ES.CONFIG.VOL.CHARA.MUTE.OFF.VDEF { @es.GSD(010,@_PINT(1))=1;return[] }
#=ES.CONFIG.VOL.CHARA.MUTE.ON.INIT
#=ES.CONFIG.VOL.CHARA.MUTE.OFF.INIT
{
	INT[@no=@_PINT(1)]
	\BT.ONOFF($BID+"VOL.CHARA.MUTE.ON" , @no, (@es.GSD(010,@no)==1)) //1=ON／0=OFF
	\BT.ONOFF($BID+"VOL.CHARA.MUTE.OFF", @no, (@es.GSD(010,@no)==0)) //OFF
		\BT.ONOFF($BID+"VOL.CHARA.FACE"    , @no, (@es.GSD(010,@no)==1)) //
	return[]
}
#=ES.CONFIG.VOL.CHARA.MUTE.ON.ON
{
	INT[@no=@_PINT(1)]
	\BT.ONOFF.GET($es.BID, @no)(@es.GSD(010,@no))
	GOSUB[#=es.SOUND.SETVOL]
		\BT.ONOFF($BID+"VOL.CHARA.FACE", @no, @es.GSD(010,@no)) //キャラアイコン
	return[]
}
#=ES.CONFIG.VOL.CHARA.MUTE.OFF.ON
{
	INT[@no=@_PINT(1)]
	\BT.ONOFF.GET($es.BID, @no)(@es.GSD(010,@no));@es.GSD(010,@no)=1-@es.GSD(010,@no)
	GOSUB[#=es.SOUND.SETVOL]
		\BT.ONOFF($BID+"VOL.CHARA.FACE", @no, @es.GSD(010,@no)) //キャラアイコン
	return[]
}

