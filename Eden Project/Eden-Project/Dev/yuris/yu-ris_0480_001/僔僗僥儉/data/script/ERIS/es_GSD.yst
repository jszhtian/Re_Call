#=es.GSD.INIT
{
	INT[@mRET]
	STR[$str]

	FILEINFO[EXIST=1 FILE=$es.SAVEPATH+"kidoku.sd" LET=@mRET]
	IF[@mRET]
	{
		LOAD[FILE="kidoku.sd" DNO=1 LET=@es.KMD()]
	}
	IFEND[]

	GOSUB[#=ES.SEL.FILELOAD]

	FILEINFO[EXIST=1 FILE=$es.SAVEPATH+"gsd.sd" LET=@mRET]
	IF[@mRET]
	{
		LOAD[FILE="gsd.sd" DNO=01 LET=@es.GSD()]
		LOAD[FILE="gsd.sd" DNO=02 LET=$es.GSD()]
		LOAD[FILE="gsd.sd" DNO=03 LET=@G()]
		LOAD[FILE="gsd.sd" DNO=04 LET=$G()]
