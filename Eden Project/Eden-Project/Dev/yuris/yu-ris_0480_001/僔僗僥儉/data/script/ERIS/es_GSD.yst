//----------------------------------------------------------------
//■演出スキップ速度計算
//----------------------------------------------------------------
#=es.CALC.DIRECTSKIPSP
{
	INT[@mSKIPSP]

	@mSKIPSP = @es.GSD(120,04) //スキップ速度
	  IF[@mSKIPSP>=192] @mSKIPSP = 2
	ELSE[@mSKIPSP>=128] @mSKIPSP = 3
	ELSE[@mSKIPSP>=064] @mSKIPSP = 5
	ELSE[@mSKIPSP>=032] @mSKIPSP = 8
	ELSE[@mSKIPSP>=016] @mSKIPSP = 12
	ELSE[@mSKIPSP>=008] @mSKIPSP = 16
//	ELSE[]              @mSKIPSP = 9999
	ELSE[]              @mSKIPSP = 32
	IFEND[]

	return[rint=@mSKIPSP]
}

