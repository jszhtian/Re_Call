	//ウィンドウサイズ反映
	//
	INT[@WSX=@es.GSD(101,01)]
	INT[@WSY=@es.GSD(101,02)]
	INT[@MAX=@es.GSD(101,03)]
	INT[@DSP=@es.GSD(101,04)]
	INT[@WLX=@es.GSD(101,05)]
	INT[@WLY=@es.GSD(101,06)]

	//微妙な誤差はスナップさせる.
	IF[@WSX>=@es.WSX-6 && @WSX<=@es.WSX+6] @WSX=@es.WSX IFEND[]
	IF[@WSY>=@es.WSY-4 && @WSY<=@es.WSY+4] @WSY=@es.WSY IFEND[]

	IF[@WSX==0] @WSX=@es.WSX IFEND[]
	IF[@WSY==0] @WSY=@es.WSY IFEND[]
	IF[@WSX< 128] @WSX=128 IFEND[]
	IF[@WSY<  16] @WSY= 16 IFEND[]
	IF[@WSX>@_SCREEN_SIZE_X] @WSX=@_SCREEN_SIZE_X IFEND[]
	IF[@WSY>@_SCREEN_SIZE_Y] @WSY=@_SCREEN_SIZE_Y IFEND[]

	IF[@_WINDOW_INIT_POS==0] //「起動時のウィンドウ位置とサイズ」が「前回終了時の状態」なら
	{
		  IF[@WLX==-9999 && @WLY==-9999] //初回起動時
		ELSE[@WLX<=-9999 || @WLY<=-9999] //最小化時 -32000,-32000
			WINDOW[NO=0 X=0 Y=0]
		ELSE[]
			WINDOW[NO=0 X=@WLX Y=@WLY]
		IFEND[]

		IF[@MAX]
			WINDOW[NO=0 WSX=@es.WSX WSY=@es.WSY]
			WINDOW[NO=0 MAXIMIZE=1]
		ELSE[]
			WINDOW[NO=0 WSX=@WSX WSY=@WSY]
			@es.WSX = @WSX
			@es.WSY = @WSY
		IFEND[]
	}
	IFEND[]

	\GSD.REFLECT

	return[]
}

//----------------------------------------------------------------
//
//■各設定一括初期化処理
//
//  その後必ず \GSD.REFRECT を通るようにする.
//
//----------------------------------------------------------------
#=es.GSD.INIT.CFG
{
	GOSUB[#=es.BT.VDEFLIST pstr="ES.CONFIG" pint2=@_PINT(1)] //ボタングループ番号
	return[]
}


//----------------------------------------------------------------
//■サウンドプリセット
//----------------------------------------------------------------
#=es.GSD.PRESET
{
	IF[@_PINT(1)==1]
	{
		@es.GSD(001,02) = 256*030/100	//音楽(BGM)音量
		@es.GSD(001,03) = 256*050/100	//効果音(SE)音量
		@es.GSD(001,04) = 256*100/100	//ボイス(VO)音量
		@es.GSD(001,13) = 256*060/100	//[環境]効果音(ESE)音量
	}
	IFEND[]
	IF[@_PINT(1)==2]
	{
		@es.GSD(001,02) = 256*050/100	//音楽(BGM)音量
		@es.GSD(001,03) = 256*070/100	//効果音(SE)音量
		@es.GSD(001,04) = 256*100/100	//ボイス(VO)音量
		@es.GSD(001,13) = 256*070/100	//[環境]効果音(ESE)音量
	}
	IFEND[]

	return[]
}


//----------------------------------------------------------------
//■
//----------------------------------------------------------------
#=es.GSD.SAVE
{
	INT[@mRET]

	//---------------------------- ウィンドウサイズ取得
	WINDOWINFO[NO=0 WSX=1 LET=@es.GSD(101,01)]
	WINDOWINFO[NO=0 WSY=1 LET=@es.GSD(101,02)]
	WINDOWINFO[NO=0 FULLSCREEN2=1 LET=@es.GSD(101,03)]
	WINDOWINFO[NO=0 DISPLAYNO=1 LET=@es.GSD(101,04)]
	WINDOWINFO[NO=0 X=1 LET=@es.GSD(101,05)]
	WINDOWINFO[NO=0 Y=1 LET=@es.GSD(101,06)]

	//---------------------------- 今回の起動時間を加算
	@es.GSD(141,03)+=@_OS_TIME(1)

	//---------------------------- 保存処理
	SAVE[DNO=01 SET=@es.GSD()]
	SAVE[DNO=02 SET=$es.GSD()]
	SAVE[DNO=03 SET=@G()]
	SAVE[DNO=04 SET=$G()]
//	SAVE[DNO=05 SET=$T(36,11)]

	GOSUB[#=es.CGMODE.FILESAVE pint=11]
	GOSUB[#=es.RPMODE.FILESAVE pint=12]
	GOSUB[#=es.MVMODE.FILESAVE pint=13]
	GOSUB[#=es.BGMMODE.FILESAVE pint=14]

	SAVE[FILE="gsd.sd"]

	GOSUB[#=ES.SEL.FILESAVE]

	FILEINFO[EXIST=1 FILE=$es.SAVEPATH+"kidoku.sd" LET=@mRET]
	IF[@mRET==0 || @es.KMD.FLAG] //ファイルが消えてるかフラグ更新されているなら
		@es.KMD.FLAG=0
		SAVE[DNO=1 SET=@es.KMD()]
		SAVE[FILE="kidoku.sd"]
	IFEND[]

	\GSD.REFLECT

	return[]
}



//----------------------------------------------------------------
//■
//----------------------------------------------------------------
#=es.GSD.REFLECT
{
	//---------------------------------------------------------------------
	// ボリュームセット
	//---------------------------------------------------------------------
	//	GOSUB[#=es.SOUND.SETVOL]

	//---------------------------------------------------------------------
	// テキスト表示速度セット
	//---------------------------------------------------------------------
		GOSUB[#=es.TX.SPD.SET pint=@es.GSD(120,01)]

	//---------------------------------------------------------------------
	// フォント反映
	//---------------------------------------------------------------------
		\_gosub(es.FONT.REFRECT)

	//---------------------------------------------------------------------
	// ウィンドウを常に手前に表示する
	//---------------------------------------------------------------------
		WINDOW[NO=0 Z=@es.GSD(110,01)]

	//---------------------------------------------------------------------
	// 非アクティブ時の動作設定
	//---------------------------------------------------------------------
		SYSTEM[BACKGROUNDEXEC=@es.GSD(110,02)]

	//---------------------------------------------------------------------
	// スクリーンセーバー防止設定
	//---------------------------------------------------------------------
		SYSTEM[SCREENSAVER=@es.GSD(110,03)]

	//---------------------------------------------------------------------
	// 演出スキップ速度
	//---------------------------------------------------------------------
		GOSUB[#=es.CALC.DIRECTSKIPSP] @es.GSD(120,05) = @_RINT(1)

	//---------------------------------------------------------------------
	// E-mote揺れスケール反映
	//---------------------------------------------------------------------
		GOSUB[#=es.EMOTE.CFG.SET]

	//---------------------------------------------------------------------
	// マウスカーソル反映
	//---------------------------------------------------------------------
	//	GOSUB[#=es.MCURSOR.SET]


	return[]
}




