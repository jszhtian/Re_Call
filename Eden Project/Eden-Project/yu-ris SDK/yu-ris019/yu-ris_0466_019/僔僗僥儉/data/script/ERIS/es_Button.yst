					@BT.ANTIME(0,1)	= @Q.I2(@_LC)
					@BT.ANTIME(0,2)	= @Q.I3(@_LC)
					@BT.ANTIME(0,3)	= @Q.I4(@_LC)
					@BT.ANTIME(0,4)	= @Q.I5(@_LC)
