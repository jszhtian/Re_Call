#=es.BT.QUE.EMPTYGET
{
	INT[@ret]
	LOOP[SET=@QUE.MAX]
	{
		IF[@Q.FLAG(@_LC)==0] @ret=@_LC;LOOPBREAK[] IFEND[]
//		IF[@Q.COM(@_LC)==0] @ret=@_LC;LOOPBREAK[] IFEND[]
	}
	LOOPEND[]

