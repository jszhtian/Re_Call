		VARACT[SET=@BT.IY()			PUSH=@no]			// 初期位置Ｙ座標
		VARACT[SET=@BT.IZ()			PUSH=@no]			// 初期位置Ｚ座標
		VARACT[SET=@BT.X()			PUSH=@no]			// Ｘ座標
		VARACT[SET=@BT.Y()			PUSH=@no]			// Ｙ座標
		VARACT[SET=@BT.Z()			PUSH=@no]			// Ｚ座標
		VARACT[SET=@BT.VAL()		PUSH=@no]			// 
