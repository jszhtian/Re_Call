				IFEND[]
	/*
				IF[@Q.COM(@_LC)==@e_PAC]
				{
					@BT.PAC.OFF(0)		= @Q.I1(@_LC)
					@BT.PAC.OVER(0)		= @Q.I2(@_LC)
