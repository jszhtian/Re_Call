#=es.BT.SET //SETADD.SET
{
	STR[$LBL  = $_PSTR(1)] // ラベル名
	INT[@LBL2 = @_PINT(2)] // ラベル名２

	IF[$LBL==""] GO[#=es.BT.SET.LEnd] IFEND[]

	INT[@wn]

	@H.COM(@e_BTSET)+=1
	$P.S1(@e_BTSET)=$LBL
	@P.I1(@e_BTSET)=@LBL2

	LOOP[SET=(48)-1]
	{
		IF[@H.COM(@_LC)>0]
		{
			GOSUB[#=es.BT.QUE.EMPTYGET];@wn=@_RINT(1)

			@Q.COM(@wn)		= @_LC
		//	$Q.NAME(@wn)	= $
			$Q.SPID(@wn)	= $LBL+"."+$(@LBL2)
			@Q.I1(@wn)		= @P.I1(@_LC)
			@Q.I2(@wn)		= @P.I2(@_LC)
			@Q.I3(@wn)		= @P.I3(@_LC)
			@Q.I4(@wn)		= @P.I4(@_LC)
			@Q.I5(@wn)		= @P.I5(@_LC)
			@Q.I6(@wn)		= @P.I6(@_LC)
			@Q.I7(@wn)		= @P.I7(@_LC)
			@Q.I8(@wn)		= @P.I8(@_LC)
			$Q.S1(@wn)		= $P.S1(@_LC)
			$Q.S2(@wn)		= $P.S2(@_LC)
			$Q.S3(@wn)		= $P.S3(@_LC)
			$Q.S4(@wn)		= $P.S4(@_LC)
			$Q.S5(@wn)		= $P.S5(@_LC)
			$Q.S6(@wn)		= $P.S6(@_LC)
			$Q.S7(@wn)		= $P.S7(@_LC)
			$Q.S8(@wn)		= $P.S8(@_LC)
			$Q.LBL(@wn)		= $LBL
			@Q.LBL2(@wn)	= @LBL2

			@Q.GUNO(@wn)	= @GUNO
			@GUNO+=1

			@Q.FLAG(@wn)	= 1
		}
		IFEND[]
	}
	LOOPEND[]

	LOOP[SET=(48)]
	{
		@H.COM(@_LC-1)=0

		@P.I1(@_LC-1)=0
		@P.I2(@_LC-1)=0
		@P.I3(@_LC-1)=0
		@P.I4(@_LC-1)=0
		@P.I5(@_LC-1)=0
		@P.I6(@_LC-1)=0
		@P.I7(@_LC-1)=0
		@P.I8(@_LC-1)=0
		$P.S1(@_LC-1)=""
		$P.S2(@_LC-1)=""
		$P.S3(@_LC-1)=""
		$P.S4(@_LC-1)=""
		$P.S5(@_LC-1)=""
		$P.S6(@_LC-1)=""
		$P.S7(@_LC-1)=""
		$P.S8(@_LC-1)=""
	}
	LOOPEND[]


	#=es.BT.SET.LEnd
//	return[]
}




#=es.BT.GO
{
	STR[$path]

	LOOP[SET=@QUE.MAX]
	{
		IF[@Q.FLAG(@_LC)==1]
		{

				IF[@Q.COM(@_LC)==@e_CG]
				{
						//	= $P.S1(@_LC)
						//	= $P.S2(@_LC)
						//	= $P.S3(@_LC)
						//	= $P.S4(@_LC)
						//	= $P.S5(@_LC)
						//	= @P.I1(@_LC)

						INT[@e]
						IF[@Q.I1(@_LC)] $path=$CGSYS.PATH ELSE[] $path=$CG.PATH IFEND[]

						$BT.FILE(0)  =$Q.S1(@_LC)+$Q.S2(@_LC)+$Q.S3(@_LC)+$Q.S4(@_LC)+$Q.S5(@_LC)
						$BT.OFF(0)   =""
						$BT.OVER(0)  =""
						$BT.ON(0)    =""
						$BT.ONOVER(0)=""
						$BT.NA(0)    =""

						IF[$Q.S1(@_LC)!="" && $Q.S2(@_LC)=="" && $Q.S3(@_LC)=="" && $Q.S4(@_LC)=="" && $Q.S5(@_LC)==""] //第２パラメータ以降が省略されていたら
						{
							//ファイル名末尾を調査し、_bt3/_bt4/_bt3n/_bt4n なら連結ボタンとして読み込む
							\_strlower($Q.S1(@_LC))
							\_strright($_RSTR(1), 4)
							IF[$_RSTR(1)=="_bt3"]
							{
								$BT.OFF(0) =$path+$Q.S1(@_LC)
								$BT.OVER(0)="*"
								$BT.ON(0)  ="*"
								@BT.SEQ(0) =3
							}
							ELSE[$_RSTR(1)=="_bt4"]
							{
								$BT.OFF(0)   =$path+$Q.S1(@_LC)
								$BT.OVER(0)  ="*"
								$BT.ON(0)    ="*"
								$BT.ONOVER(0)="*"
								@BT.SEQ(0)   =4
							}
							ELSE[]
							{
								//_off, _over, _on, _onover, _na を勝手に探しにいく.(※png固定)
								$BT.OFF(0)=$path+$Q.S1(@_LC)
								FILEINFO[EXIST=1 FILE=$path+$Q.S1(@_LC)+"_off.png"    LET=@e];IF[@e] $BT.OFF(0)    = $path+$Q.S1(@_LC)+"_off"    IFEND[] // OFF時の画像
								FILEINFO[EXIST=1 FILE=$path+$Q.S1(@_LC)+"_over.png"   LET=@e];IF[@e] $BT.OVER(0)   = $path+$Q.S1(@_LC)+"_over"   IFEND[] // OVER時の画像
								FILEINFO[EXIST=1 FILE=$path+$Q.S1(@_LC)+"_on.png"     LET=@e];IF[@e] $BT.ON(0)     = $path+$Q.S1(@_LC)+"_on"     IFEND[] // ON時の画像
								FILEINFO[EXIST=1 FILE=$path+$Q.S1(@_LC)+"_onover.png" LET=@e];IF[@e] $BT.ONOVER(0) = $path+$Q.S1(@_LC)+"_onover" IFEND[] // ONOVER時の画像
								FILEINFO[EXIST=1 FILE=$path+$Q.S1(@_LC)+"_na.png"     LET=@e];IF[@e] $BT.NA(0)     = $path+$Q.S1(@_LC)+"_na"     IFEND[] // NA時の画像
							}
							IFEND[]

						}
						ELSE[]
						{
							IF[$Q.S1(@_LC)!=""] $BT.OFF(0)    = $path+$Q.S1(@_LC) IFEND[] // OFF時の画像
							IF[$Q.S2(@_LC)!=""] $BT.OVER(0)   = $path+$Q.S2(@_LC) IFEND[] // OVER時の画像
							IF[$Q.S3(@_LC)!=""] $BT.ON(0)     = $path+$Q.S3(@_LC) IFEND[] // ON時の画像
							IF[$Q.S4(@_LC)!=""] $BT.ONOVER(0) = $path+$Q.S4(@_LC) IFEND[] // ONOVER時の画像
							IF[$Q.S5(@_LC)!=""] $BT.NA(0)     = $path+$Q.S5(@_LC) IFEND[] // NA時の画像
						}
						IFEND[]

						IF[$BT.OFF(0)==""] \_mes("ボタン画像指定が間違っています。") END[] IFEND[]

						@BT.A(0)			= 256 // 不透明度(0〜256)
						@BT.ES(0)			=   1 // 可視不可視(1/0)
						@BT.ES2(0)			=   1 // 可視不可視2(1/0)
						@BT.N(0)			=   0 // NA有効無効(1/0)
						@BT.W(0)			=   1 // 機能有効無効(1/0)
						@BT.MST(0)			=   0 // 
						@BT.OLD.MST(0)		=   0 // 

						@BT.ANTM(0)			=   0 // アニメ・現在の時間

						LOOP[SET=6]
						{
							@BT.ANCM(0,@_LC-1)		=   1 // アニメ・現在のコマ
							@BT.ANNUM(0,@_LC-1)		=   1 // アニメ枚数
							@BT.ANTIME(0,@_LC-1)	=   0 // アニメコマ時間
							@BT.ANLPTYPE(0,@_LC-1)	=   0 // アニメループ種類
						}
						LOOPEND[]

					//	@BT.CGLOAD(0)		=   1 // ＣＧを先読みしておくか否か
						@BT.SW(0)			=   0 // ボタンON/OFF
						@BT.STATUS(0)		=	0 // ボタン描画状態
				}
				IFEND[]

				IF[@Q.COM(@_LC)==@e_CGAN]
				{
						//	= $P.S1(@_LC)
						//	= $P.S2(@_LC)
						//	= $P.S3(@_LC)
						//	= $P.S4(@_LC)
						//	= $P.S5(@_LC)
						//	= $P.S6(@_LC)

					//	IF[@_PINT(4)] $path=$CGSYS.PATH ELSE[] $path=$CG.PATH IFEND[]
						$path=$CGSYS.PATH

						$BT.FILE(0)  =$Q.S1(@_LC)+$Q.S2(@_LC)+$Q.S3(@_LC)+$Q.S4(@_LC)+$Q.S5(@_LC)
						$BT.OFF(0)   =""
						$BT.OVER(0)  =""
						$BT.ON(0)    =""
						$BT.ONOVER(0)=""
						$BT.NA(0)    =""

						IF[$Q.S1(@_LC)!=""] $BT.OFF(0)    = $path+$Q.S1(@_LC) IFEND[] // OFF時の画像
						IF[$Q.S2(@_LC)!=""] $BT.OVER(0)   = $path+$Q.S2(@_LC) IFEND[] // OVER時の画像
						IF[$Q.S3(@_LC)!=""] $BT.ON(0)     = $path+$Q.S3(@_LC) IFEND[] // ON時の画像
						IF[$Q.S4(@_LC)!=""] $BT.ONOVER(0) = $path+$Q.S4(@_LC) IFEND[] // ONOVER時の画像
						IF[$Q.S5(@_LC)!=""] $BT.NA(0)     = $path+$Q.S5(@_LC) IFEND[] // NA時の画像

						IF[$BT.OFF(0)==""] \_mes("ボタン画像指定が間違っています。") END[] IFEND[]

						@BT.A(0)			= 256 // 不透明度(0〜256)
						@BT.ES(0)			=   1 // 可視不可視(1/0)
						@BT.ES2(0)			=   1 // 可視不可視2(1/0)
						@BT.N(0)			=   0 // NA有効無効(1/0)
						@BT.W(0)			=   1 // 機能有効無効(1/0)
						@BT.MST(0)			=   0 // 
						@BT.OLD.MST(0)		=   0 // 

						@BT.ANTM(0)			=   0 // アニメ・現在の時間

						LOOP[SET=6]
						{
							@BT.ANCM(0,@_LC-1)		=   1 // アニメ・現在のコマ
							@BT.ANNUM(0,@_LC-1)		=   1 // アニメ枚数
							@BT.ANTIME(0,@_LC-1)	=   0 // アニメコマ時間
							@BT.ANLPTYPE(0,@_LC-1)	=   0 // アニメループ種類
						}
						LOOPEND[]

					//	@BT.CGLOAD(0)		=   1 // ＣＧを先読みしておくか否か
						@BT.SW(0)			=   0 // ボタンON/OFF
						@BT.STATUS(0)		=	0 // ボタン描画状態
				}
				IFEND[]

				IF[@Q.COM(@_LC)==@e_NAME]		$BT.NAME(0)		= $Q.S1(@_LC)	IFEND[] // ボタン名
				IF[@Q.COM(@_LC)==@e_A]			@BT.A(0)		= @Q.I1(@_LC)	IFEND[] // 可視不可視
				IF[@Q.COM(@_LC)==@e_E]			@BT.ES(0)		= @Q.I1(@_LC)	IFEND[] // 可視不可視
				IF[@Q.COM(@_LC)==@e_E2]			@BT.ES2(0)		= @Q.I1(@_LC)	IFEND[] // 可視不可視
				IF[@Q.COM(@_LC)==@e_N]			@BT.N(0)		= @Q.I1(@_LC)	IFEND[] // NA状態
				IF[@Q.COM(@_LC)==@e_T]			@BT.TMAX(0)		= @Q.I1(@_LC)	IFEND[] // ON時間
				IF[@Q.COM(@_LC)==@e_W]			@BT.W(0)		= @Q.I1(@_LC)	IFEND[] // 機能有効無効
				IF[@Q.COM(@_LC)==@e_X]			@BT.X(0)		= @Q.I1(@_LC)	IFEND[] // Ｘ座標
				IF[@Q.COM(@_LC)==@e_Y]			@BT.Y(0)		= @Q.I1(@_LC)	IFEND[] // Ｙ座標
				IF[@Q.COM(@_LC)==@e_Z]			@BT.Z(0)		= @Q.I1(@_LC)	IFEND[] // Ｚ座標
				IF[@Q.COM(@_LC)==@e_SX]			@BT.SX(0)		= @Q.I1(@_LC)	IFEND[] // サイズ指定Ｘ(0=デフォルトサイズ)
				IF[@Q.COM(@_LC)==@e_SY]			@BT.SY(0)		= @Q.I1(@_LC)	IFEND[] // サイズ指定Ｙ(0=デフォルトサイズ)
				IF[@Q.COM(@_LC)==@e_PX]			@BT.PX(0)		= @Q.I1(@_LC)	IFEND[] // サイズ指定Ｘ(100=デフォルトサイズ)
				IF[@Q.COM(@_LC)==@e_PY]			@BT.PY(0)		= @Q.I1(@_LC)	IFEND[] // サイズ指定Ｙ(100=デフォルトサイズ)
				IF[@Q.COM(@_LC)==@e_RADIO]		@BT.RADIO(0)	= @Q.I1(@_LC)	IFEND[] // グループ番号
				IF[@Q.COM(@_LC)==@e_MODE]		@BT.MODE(0)		= @Q.I1(@_LC)	IFEND[] // 描画モード
				IF[@Q.COM(@_LC)==@e_TYPE]		@BT.TYPE(0)		= @Q.I1(@_LC)	IFEND[] // ボタン種類
				IF[@Q.COM(@_LC)==@e_BTFLAG]		@BT.BTFLAG(0)	= @Q.I1(@_LC)	IFEND[] // 
				IF[@Q.COM(@_LC)==@e_VAL]		@BT.VAL(0)		= @Q.I1(@_LC)	IFEND[] // 戻り値
				IF[@Q.COM(@_LC)==@e_TH]			@BT.TH(0)		= @Q.I1(@_LC)	IFEND[] // スルー
				IF[@Q.COM(@_LC)==@e_MAPA]		@BT.MAP.A(0)	= @Q.I1(@_LC)	IFEND[] // 
				IF[@Q.COM(@_LC)==@e_MAPCG]		$BT.MASK(0)		= $Q.S1(@_LC)	IFEND[] // MASK画像

				IF[@Q.COM(@_LC)==@e_KEY]
				{
					@BT.KEY(0,0) = @Q.I1(@_LC)
					@BT.KEY(0,1) = @Q.I2(@_LC)
					@BT.KEY(0,2) = @Q.I3(@_LC)
					@BT.KEY(0,3) = @Q.I4(@_LC)
					@BT.KEY(0,4) = @Q.I5(@_LC)
					@BT.KEY(0,5) = @Q.I6(@_LC)
					@BT.KEY(0,6) = @Q.I7(@_LC)
					@BT.KEY(0,7) = @Q.I8(@_LC)
				}
				IFEND[]
				IF[@Q.COM(@_LC)==@e_KEYS]
				{
					@BT.KEY.S(0,0) = @Q.I1(@_LC)
					@BT.KEY.S(0,1) = @Q.I2(@_LC)
					@BT.KEY.S(0,2) = @Q.I3(@_LC)
					@BT.KEY.S(0,3) = @Q.I4(@_LC)
					@BT.KEY.S(0,4) = @Q.I5(@_LC)
					@BT.KEY.S(0,5) = @Q.I6(@_LC)
					@BT.KEY.S(0,6) = @Q.I7(@_LC)
					@BT.KEY.S(0,7) = @Q.I8(@_LC)
				}
				IFEND[]
				IF[@Q.COM(@_LC)==@e_KEYNUM]
				{
					@BT.KEY.NUM(0,0) = @Q.I1(@_LC)
					@BT.KEY.NUM(0,1) = @Q.I2(@_LC)
					@BT.KEY.NUM(0,2) = @Q.I3(@_LC)
					@BT.KEY.NUM(0,3) = @Q.I4(@_LC)
					@BT.KEY.NUM(0,4) = @Q.I5(@_LC)
					@BT.KEY.NUM(0,5) = @Q.I6(@_LC)
					@BT.KEY.NUM(0,6) = @Q.I7(@_LC)
					@BT.KEY.NUM(0,7) = @Q.I8(@_LC)
