				IFEND[]

				IF[@Q.COM(@_LC)==@e_CURSOR.START]
					@BT.C.STARTFLAG = @Q.I1(@_LC) // カーソル開始ボタン
				IFEND[]

				IF[@Q.COM(@_LC)==@e_ANNUM]
				{
					@BT.ANNUM(0,0)	= @Q.I1(@_LC)
					@BT.ANNUM(0,1)	= @Q.I2(@_LC)
