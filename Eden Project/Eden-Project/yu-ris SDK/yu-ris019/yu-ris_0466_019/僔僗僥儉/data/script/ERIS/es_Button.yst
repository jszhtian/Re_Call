{
	STR[$s(4)]
	INT[@mLC]

	@H.COM(@e_KEY)+=1
	@H.COM(@e_KEYNUM)+=1

	LOOP[SET=(8)]
	{
		@mLC=@_LC
		IF[@mLC==1]
			@P.I1(@e_KEY)=0
			@P.I1(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==2]
			@P.I2(@e_KEY)=0
			@P.I2(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==3]
			@P.I3(@e_KEY)=0
			@P.I3(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==4]
			@P.I4(@e_KEY)=0
			@P.I4(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==5]
			@P.I5(@e_KEY)=0
			@P.I5(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==6]
			@P.I6(@e_KEY)=0
			@P.I6(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==7]
			@P.I7(@e_KEY)=0
			@P.I7(@e_KEYNUM)=0
		IFEND[]
		IF[@mLC==8]
			@P.I8(@e_KEY)=0
			@P.I8(@e_KEYNUM)=0
		IFEND[]

		\_strsplit.str($_PSTR(@mLC), "+");

		LOOP[SET=(4)] $s(@_LC-1)=$_RSTR(@_LC) LOOPEND[]

		LOOP[SET=(4)]
		{
			\_strtocode($s(@_LC-1))

//			@BT.KEY(0,@mLC-1) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
//			IF[@_RINT(1)>0] @BT.KEY.NUM(0,@mLC-1) = @_LC IFEND[]

			IF[@mLC==1]
				@P.I1(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I1(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==2]
				@P.I2(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I2(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==3]
				@P.I3(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I3(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==4]
				@P.I4(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I4(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==5]
				@P.I5(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I5(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==6]
				@P.I6(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I6(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==7]
				@P.I7(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I7(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]
			IF[@mLC==8]
				@P.I8(@e_KEY) += @_RINT(1)*((@_LC==4)*@es.POW2(48)+(@_LC==3)*@es.POW2(32)+(@_LC==2)*@es.POW2(16)+(@_LC==1))
				IF[@_RINT(1)>0] @P.I8(@e_KEYNUM) = @_LC IFEND[]
			IFEND[]

		}
		LOOPEND[]
	}
	LOOPEND[]

	return[]
}


