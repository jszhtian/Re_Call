//▼ボタンON/OFF @@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
//
#=es.BT.ONOFF.SET
{
	STR[$ID  =$_PSTR(1)]
	INT[@NO  =@_PINT(2)]
	INT[@flag=@_PINT(3)]
	INT[@no]
	INT[@RNO]
	\BT.NO.GET(@no, $ID, @NO)

	IF[@flag==1]
	{
		// ラジオボタン
		IF[@BT.RADIO(@no)>0]
		{
			//同じグループのON状態を解除.
			@RNO = @BT.RADIO(@no)
			LOOP[SET=@BT.NUM]
			{
				//ボタン有効かつ同じグループ番号なら.
				IF[(@BT.ES(@_LC) & @BT.ES2(@_LC))==1 && @RNO==@BT.RADIO(@_LC)]
				{
					@BT.SW(@_LC) = 0
				}
				IFEND[]
			}
			LOOPEND[]
		}
		IFEND[]

		IF[$BT.ON(@no)!=""]
		{
			@BT.SW(@no) = 1
		}
		IFEND[]
	}
	ELSE[]
	{
		@BT.SW(@no) = 0
	}
	IFEND[]

	return[]
}

