					@BT.PAC.ONOVER(0)	= @Q.I4(@_LC)
					@BT.PAC.NA(0)		= @Q.I5(@_LC)
				}
				IFEND[]
	*/
				IF[@Q.COM(@_LC)==@e_SE]
					$BT.SSE.OVER(0)	= $Q.S1(@_LC)
					$BT.SSE.ON(0)	= $Q.S2(@_LC)
				IFEND[]

				IF[@Q.COM(@_LC)==@e_CURSOR.UP]
					$BT.C.UP(0)		= $Q.S1(@_LC) // 上
					@BT.C.UP2(0)	= @Q.I1(@_LC) // 上
				IFEND[]

				IF[@Q.COM(@_LC)==@e_CURSOR.DOWN]
					$BT.C.DOWN(0)	= $Q.S1(@_LC) // 下
					@BT.C.DOWN2(0)	= @Q.I1(@_LC) // 下
				IFEND[]

				IF[@Q.COM(@_LC)==@e_CURSOR.LEFT]
					$BT.C.LEFT(0)	= $Q.S1(@_LC) // 左
